{
    "name": "laraeast/laravel-locales",
    "description": "The package used to support multi locales in your application.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Aliraqi",
            "email": "aliraqi.dev@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "~5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0|~4.0|~5.0|~6.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Laraeast\\LaravelLocales\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraeast\\LaravelLocales\\Providers\\LocalesServiceProvider"
            ],
            "aliases": {
                "Locales": "Laraeast\\LaravelLocales\\Facades\\Locales"
            }
        }
    }
}
